        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.4.0",
        "jest": "^29.6.3",
        "lint-staged": "^13.2.1",
        "prettier": "^2.2.1",
        "ts-jest": "^29.1.1",
