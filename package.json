{
    "author": {
        "email": "ryansonshine@users.noreply.github.com",
        "name": "",
        "url": "https://github.com/ryansonshine"
    },
    "bugs": {
        "url": "https://github.com/ryansonshine//issues"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/@ryansonshine/cz-conventional-changelog"
        }
    },
    "description": "A template for creating npm packages using TypeScript and VSCode",
    "devDependencies": {
        "@jest/globals": "^29.6.3",
        "@ryansonshine/commitizen": "^4.2.8",
        "@ryansonshine/cz-conventional-changelog": "^3.3.4",
        "@swc-node/register": "^1.6.7",
        "@types/jest": "^29.5.4",
        "@types/node": "^12.20.11",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "conventional-changelog-conventionalcommits": "^5.0.0",
        "eslint": "^7.25.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.4.0",
        "jest": "^29.6.3",
        "lint-staged": "^13.2.1",
        "prettier": "^2.2.1",
        "semantic-release": "^21.0.1",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.2.1",
        "typescript": "^4.2.4"
    },
    "engines": {
        "node": ">=12.0"
    },
    "files": [
        "index.d.ts",
        "index.ts",
        "lib/**/*"
    ],
    "homepage": "https://github.com/ryansonshine/#readme",
    "keywords": [
        "boilerplate",
        "template",
        "typescript",
        "vscode",
        "commitizen",
        "semantic-release",
        "codecov"
    ],
    "license": "MIT",
    "lint-staged": {
        "*.ts": "eslint --cache --cache-location .eslintcache --fix"
    },
    "main": "./lib/index.js",
    "name": "memoryts",
    "release": {
        "branches": [
            "main"
        ],
        "plugins": [
            [
                "@semantic-release/commit-analyzer",
                {
                    "preset": "conventionalcommits",
                    "releaseRules": [
                        {
                            "release": "patch",
                            "scope": "deps",
                            "type": "build"
                        }
                    ]
                }
            ],
            [
                "@semantic-release/release-notes-generator",
                {
                    "preset": "conventionalcommits",
                    "presetConfig": {
                        "types": [
                            {
                                "section": "Features",
                                "type": "feat"
                            },
                            {
                                "section": "Bug Fixes",
                                "type": "fix"
                            },
                            {
                                "hidden": false,
                                "section": "Dependencies and Other Build Updates",
                                "type": "build"
                            }
                        ]
                    }
                }
            ],
            "@semantic-release/npm",
            "@semantic-release/github"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ryansonshine/.git"
    },
    "scripts": {
        "build": "tsc --project tsconfig.build.json",
        "clean": "rm -rf ./lib/",
        "cm": "cz",
        "lint": "eslint ./src/ --fix",
        "semantic-release": "semantic-release",
        "test": "jest",
        "typecheck": "tsc --noEmit"
    },
    "version": "0.0.0-development"
}
